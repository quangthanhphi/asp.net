name: dockercompose15867254620216380152
services:
  electromvc:
    build:
      context: /Users/phiquangthanh/Desktop/ElectroMVC
      dockerfile: ElectroMVC/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: ElectroMVC
      target: base
    container_name: ElectroMVC
    entrypoint:
    - tail
    - -f
    - /dev/null
