    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net7.0/ElectroMVC.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: /Users/phiquangthanh/Desktop/ElectroMVC/ElectroMVC
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: /Users/phiquangthanh/vsdbg/vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: /Users/phiquangthanh/.aspnet/https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/phiquangthanh/.microsoft/usersecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/phiquangthanh/.nuget/packages/
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: /Users/phiquangthanh/Desktop/ElectroMVC
      target: /src
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose15867254620216380152_default